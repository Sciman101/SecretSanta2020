code = "shader_type canvas_item;

void fragment() {
	
	vec2 pos = UV;
	pos.y = mod(pos.y - TIME*0.25,0.125);
	vec4 c = texture(TEXTURE,pos);
	pos.y = mod(pos.y - TIME*0.35,0.125);
	c = mix(c,texture(TEXTURE,pos),0.5);
	
	COLOR = c;
}"

